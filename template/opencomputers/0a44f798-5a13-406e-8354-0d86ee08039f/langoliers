os.execute("cls")
function pLang()
  io.write('\n')
  io.write('_|                                                _|  _|                                \n')
  io.write('_|          _|_|_|  _|_|_|      _|_|_|    _|_|    _|        _|_|    _|  _|_|    _|_|_|  \n')
  io.write('_|        _|    _|  _|    _|  _|    _|  _|    _|  _|  _|  _|_|_|_|  _|_|      _|_|      \n')
  io.write('_|        _|    _|  _|    _|  _|    _|  _|    _|  _|  _|  _|        _|            _|_|  \n')
  io.write('_|_|_|_|    _|_|_|  _|    _|    _|_|_|    _|_|    _|  _|    _|_|_|  _|        _|_|_|    \n')
  io.write('                                    _|                                                  \n')
  io.write('                                _|_|                                                    \n')
  io.write('\n')
end

function pError()
  os.execute("cls")
  io.write('_|_|_|_|                                          _|  _|  \n')
  io.write('_|        _|  _|_|  _|  _|_|    _|_|    _|  _|_|  _|  _|  \n')
  io.write('_|_|_|    _|_|      _|_|      _|    _|  _|_|      _|  _|  \n')
  io.write('_|        _|        _|        _|    _|  _|                \n')
  io.write('_|_|_|_|  _|        _|          _|_|    _|        _|  _|  \n')
  io.write('\n')
  local computer = require("computer")
  computer.beep()
end

function loading()
